{
    "name": "jms/security-extra-bundle",
    "description": "Enhances the Symfony2 Security Component by adding several new features",
    "keywords": ["annotations","authorization","expression","secure","security"],
    "homepage": "http://jmsyst.com/bundles/JMSSecurityExtraBundle",
    "type": "symfony-bundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ruudk/JMSDiExtraBundle"
        }
    ],
    "require": {
        "php": "~5.6|~7.0",
        "symfony/framework-bundle": "~2.7|~3.0|~4.0",
        "symfony/security-bundle": "~2.7|~3.0|~4.0",
        "symfony/security-acl": "~2.7|~3.0|~4.0",
        "symfony/dependency-injection": "~2.7|~3.0|~4.0",
        "symfony/routing": "~2.7|~3.0|~4.0",
        "jms/metadata": "~1.0",
        "jms/parser-lib": "~1.0",
        "jms/aop-bundle": "~1.0",
        "jms/di-extra-bundle": "dev-symfony-4",
        "symfony/symfony": "4.0.*"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "~2.7|~3.0|~4.0|~5.0",
        "symfony/class-loader": "~2.7|~3.0|~4.0",
        "symfony/yaml": "~2.7|~3.0|~4.0",
        "symfony/browser-kit": "~2.7|~3.0|~4.0",
        "symfony/finder": "~2.7|~3.0|~4.0",
        "symfony/css-selector": "~2.7|~3.0|~4.0",
        "symfony/process": "~2.7|~3.0|~4.0",
        "doctrine/doctrine-bundle": "~1.0",
        "symfony/twig-bundle": "~2.7|~3.0|~4.0",
        "symfony/twig-bridge": "^2.7|~3.0|~4.0",
        "twig/twig": "^1.23.1|~2.0",
        "doctrine/orm": "~2.3",
        "doctrine/common": "~2.3",
        "symfony/form": "~2.7|~3.0|~4.0",
        "symfony/validator": "~2.7|~3.0|~4.0",
        "symfony/phpunit-bridge": "~2.7|~3.0|~4.0"
    },
    "autoload": {
        "psr-4": { "JMS\\SecurityExtraBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "JMS\\SecurityExtraBundle\\Tests\\": "tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6-dev"
        }
    }
}
