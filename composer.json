{
    "name": "jms/security-extra-bundle",
    "description": "Enhances the Symfony2 Security Component by adding several new features",
    "keywords": ["annotations","authorization","expression","secure","security"],
    "homepage": "http://jmsyst.com/bundles/JMSSecurityExtraBundle",
    "type": "symfony-bundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^5.5|~7.0",
        "symfony/framework-bundle": "~2.8|3.0",
        "symfony/security-bundle": "~2.8|3.0",
        "jms/metadata": "~1.0",
        "jms/parser-lib": "~1.0",
        "jms/aop-bundle": "~1.0",
        "jms/di-extra-bundle": "~1.3"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "~2.8|3.0",
        "symfony/class-loader": "~2.8|3.0",
        "symfony/yaml": "~2.8|3.0",
        "symfony/browser-kit": "~2.8|3.0",
        "symfony/finder": "~2.8|3.0",
        "symfony/css-selector": "~2.8|3.0",
        "symfony/process": "~2.8|3.0",
        "doctrine/doctrine-bundle": "~1.6",
        "symfony/twig-bundle": "~2.8|3.0",
        "doctrine/orm": "~2.5",
        "symfony/form": "~2.8|3.0",
        "symfony/validator": "~2.8|3.0"
    },
    "autoload": {
        "psr-0": { "JMS\\SecurityExtraBundle": "" }
    },
    "target-dir": "JMS/SecurityExtraBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
