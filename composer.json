{
    "name": "jms/security-extra-bundle",
    "description": "Enhances the Symfony2 Security Component by adding several new features",
    "keywords": ["annotations","authorization","expression","secure","security"],
    "homepage": "http://jmsyst.com/bundles/JMSSecurityExtraBundle",
    "type": "symfony-bundle",
    "license": "Apache2",
    "authors": [
        {
            "name": "Johannes M. Schmitt",
            "email": "schmittjoh@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "~5.3|~7.0",
        "symfony/framework-bundle": "~2.6|~3.0",
        "symfony/security-bundle": "~2.6|~3.0",
        "symfony/security-acl": "~2.6|~3.0",
        "jms/metadata": "~1.0",
        "jms/parser-lib": "~1.0",
        "jms/aop-bundle": "~1.2",
        "jms/di-extra-bundle": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.6,<6",
        "sensio/framework-extra-bundle": "*",
        "symfony/class-loader": "*",
        "symfony/yaml": "*",
        "symfony/browser-kit": "*",
        "symfony/finder": "*",
        "symfony/css-selector": "*",
        "symfony/process": "*",
        "doctrine/doctrine-bundle": "*",
        "symfony/twig-bundle": "*",
        "doctrine/orm": "*",
        "symfony/form": "*",
        "symfony/validator": "*"
    },
    "autoload": {
        "psr-0": { "JMS\\SecurityExtraBundle": "" }
    },
    "target-dir": "JMS/SecurityExtraBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
